#################################################################
# CEFR Defined Terms (overall + Native)
#################################################################
:CEFR a schema:DefinedTermSet ;
  schema:name "Common European Framework of Reference for Languages" ;
  schema:description "CEFR proficiency levels for languages (plus a Native term for mother tongue)." .

:A1 a schema:DefinedTerm ; schema:name "A1" ; schema:inDefinedTermSet :CEFR .
:A2 a schema:DefinedTerm ; schema:name "A2" ; schema:inDefinedTermSet :CEFR .
:B1 a schema:DefinedTerm ; schema:name "B1" ; schema:inDefinedTermSet :CEFR .
:B2 a schema:DefinedTerm ; schema:name "B2" ; schema:inDefinedTermSet :CEFR .
:C1 a schema:DefinedTerm ; schema:name "C1" ; schema:inDefinedTermSet :CEFR .
:C2 a schema:DefinedTerm ; schema:name "C2" ; schema:inDefinedTermSet :CEFR .
:Native a schema:DefinedTerm ; schema:name "Native (mother tongue)" ; schema:inDefinedTermSet :CEFR .

# Custom proficiency pattern
:LanguageProficiency a owl:Class ; rdfs:label "Language Proficiency" .
:hasLanguageProficiency a owl:ObjectProperty ;
  rdfs:domain schema:Person ; rdfs:range :LanguageProficiency .
:languageName a owl:DatatypeProperty ;
  rdfs:domain :LanguageProficiency ; rdfs:range xsd:string .
:cefrLevel a owl:ObjectProperty ;
  rdfs:domain :LanguageProficiency ; rdfs:range schema:DefinedTerm .

# Overall levels (English C1; Finnish A1; Bengali Native)
:engC1 a :LanguageProficiency ; :languageName "English" ; :cefrLevel :C1 .
:finA1 a :LanguageProficiency ; :languageName "Finnish" ; :cefrLevel :A1 .
:benNative a :LanguageProficiency ; :languageName "Bengali" ; :cefrLevel :Native .

