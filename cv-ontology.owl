#################################################################
# Employment roles (OrganizationRole pattern)
#################################################################
:role-ucb-exec a schema:EmployeeRole ;
  schema:roleName "Executive Officer" ;
  schema:startDate "2018-01-03"^^xsd:date ;
  schema:endDate "2025-01-23"^^xsd:date ;
  schema:memberOf :UCB .

:role-ucb-access a schema:EmployeeRole ;
  schema:roleName "Access Control Analyst" ;
  schema:startDate "2021-11-01"^^xsd:date ;
  schema:endDate "2025-01-23"^^xsd:date ;
  schema:memberOf :UCB .

:role-scholastica-teacher a schema:EmployeeRole ;
  schema:roleName "Computer Science Teacher (IGCSE O/A Levels)" ;
  schema:startDate "2012-08-13"^^xsd:date ;
  schema:endDate "2017-06-08"^^xsd:date ;
  schema:memberOf :Scholastica .

:role-nsu-ta a schema:EmployeeRole ;
  schema:roleName "Undergraduate Teaching Assistant" ;
  schema:startDate "2011-01-01"^^xsd:date ;
  schema:endDate "2011-12-31"^^xsd:date ;
