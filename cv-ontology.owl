@base <https://<your-username>.github.io/cv-ontology.owl> .

<https://<your-username>.github.io/cv-ontology.owl> a owl:Ontology ;
  rdfs:label "CV Ontology and Data for M Jahidul Islam" ;
  dcterms:creator "M Jahidul Islam" ;
  owl:imports <http://xmlns.com/foaf/0.1/> ,
              <http://schema.org/> ,
              <http://purl.org/dc/terms/> .

#################################################################
# Core person
#################################################################
:jahid a foaf:Person , schema:Person ;
  schema:name "M Jahidul Islam" ;
  schema:nationality [ a schema:Country ; schema:name "Bangladesh" ; schema:sameAs <https://dbpedia.org/resource/Bangladesh> ] ;
  schema:knowsLanguage "Bengali", "English C1", "Finnish A1" ;
  schema:url <https://<your-username>.github.io/cv.html> .

